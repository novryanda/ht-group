generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
  engineType    = "library"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Company {
  id                String              @id @default(cuid())
  code              String?             @unique
  name              String              @unique
  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt
  accounts          Account[]
  fiscalPeriods     FiscalPeriod[]
  journalEntries    JournalEntry[]
  openingBalances   OpeningBalance[]
  systemAccountMaps SystemAccountMap[]
  WeighbridgeTicket WeighbridgeTicket[]
  employees         Employee[]
}

model Employee {
  id_karyawan           String           @id @default(cuid())
  nama                  String?          @db.VarChar(100)
  status_kk             String?          @db.VarChar(50)
  jenis_kelamin         String?          @db.Char(1)
  agama                 String?          @db.VarChar(30)
  suku                  String?          @db.VarChar(50)
  golongan_darah        String?          @db.VarChar(5)
  no_telp_hp            String?          @db.VarChar(20)
  tempat_lahir          String?          @db.VarChar(50)
  tanggal_lahir         DateTime?
  umur                  Int?
  alamat_rt_rw          String?          @db.VarChar(20)
  alamat_desa           String?          @db.VarChar(100)
  alamat_kecamatan      String?          @db.VarChar(100)
  alamat_kabupaten      String?          @db.VarChar(100)
  alamat_provinsi       String?          @db.VarChar(100)
  pendidikan_terakhir   String?          @db.VarChar(50)
  jurusan               String?          @db.VarChar(100)
  jabatan               String?          @db.VarChar(100)
  devisi                String?          @db.VarChar(100)
  level                 String?          @db.VarChar(50)
  tgl_masuk_kerja       DateTime?
  tgl_terakhir_kerja    DateTime?
  masa_kerja            String?          @db.VarChar(50)
  status_pkwt           String?          @db.VarChar(50)
  no_bpjs_tenaga_kerja  String?          @db.VarChar(50)
  no_nik_ktp            String?          @db.VarChar(20)
  no_bpjs_kesehatan     String?          @db.VarChar(50)
  no_npwp               String?          @db.VarChar(50)
  status_pajak          String?          @db.VarChar(20)
  no_rekening_bank      String?          @db.VarChar(50)
  perusahaan_sebelumnya String?          @db.VarChar(100)
  companyId             String?
  userId                String?          @unique
  createdAt             DateTime         @default(now())
  updatedAt             DateTime         @updatedAt
  EmployeeFamily        EmployeeFamily[]
  company               Company?         @relation(fields: [companyId], references: [id])
  user                  User?            @relation(fields: [userId], references: [id])

  @@map("karyawan")
}

model EmployeeFamily {
  id                String         @id @default(cuid())
  employeeId        String
  nama              String         @db.VarChar(100)
  hubungan          FamilyRelation
  jenis_kelamin     String?        @db.Char(1)
  tanggal_lahir     DateTime?
  umur              Int?
  no_nik_ktp        String?        @db.VarChar(20)
  no_bpjs_kesehatan String?        @db.VarChar(50)
  no_telp_hp        String?        @db.VarChar(20)
  createdAt         DateTime       @default(now())
  updatedAt         DateTime       @updatedAt
  employee          Employee       @relation(fields: [employeeId], references: [id_karyawan], onDelete: Cascade)

  @@index([employeeId])
  @@map("employee_family")
}

model User {
  id        String    @id @default(cuid())
  name      String?
  email     String?   @unique
  password  String?
  role      UserRole  @default(OPERATOR)
  createdAt DateTime  @default(now())
  updatedAt DateTime  @default(now()) @updatedAt
  employee  Employee?
}

model SupplierTBS {
  id                    String              @id @default(cuid())
  nomorForm             String?
  typeSupplier          SupplierType
  pajakPKP              PajakPKP            @default(NON_PKP)
  namaPemilik           String
  alamatPemilik         String?
  hpPemilik             String?
  namaPerusahaan        String?
  alamatRampPeron       String?
  hpPerusahaan          String?
  bujur                 String?
  lintang               String?
  profilKebun           Json?
  pengelolaanSwadaya    String?
  pengelolaanKelompok   String?
  pengelolaanPerusahaan String?
  jenisBibit            String?
  sertifikasiISPO       Boolean             @default(false)
  sertifikasiRSPO       Boolean             @default(false)
  aktePendirian         String?
  aktePerubahan         String?
  nib                   String?             @unique
  siup                  String?
  npwp                  String?             @unique
  penjualanLangsungPKS  String?
  penjualanAgen         String?
  transportMilikSendiri Int?
  transportPihak3       Int?
  bankName              String?
  bankAccountNo         String?
  bankAccountName       String?
  createdAt             DateTime            @default(now())
  updatedAt             DateTime            @updatedAt
  weighbridgeTickets    WeighbridgeTicket[]

  @@index([typeSupplier])
  @@index([pajakPKP])
}

model Buyer {
  id              String         @id @default(cuid())
  buyerCode       String         @unique
  type            BuyerType
  legalName       String
  tradeName       String?
  npwp            String?        @unique
  pkpStatus       PkpStatus
  addressLine     String
  city            String
  province        String
  postalCode      String?
  billingEmail    String
  phone           String
  destinationName String
  destinationAddr String
  status          BuyerStatus    @default(VERIFIED)
  verifiedAt      DateTime?
  verifiedById    String?
  createdAt       DateTime       @default(now())
  updatedAt       DateTime       @updatedAt
  contacts        BuyerContact[]
  docs            BuyerDoc[]

  @@index([type])
  @@index([status])
  @@index([city])
  @@index([province])
}

model BuyerContact {
  id        String  @id @default(cuid())
  buyerId   String
  name      String
  role      String?
  email     String
  phone     String
  isBilling Boolean @default(false)
  buyer     Buyer   @relation(fields: [buyerId], references: [id], onDelete: Cascade)

  @@index([buyerId])
}

model BuyerDoc {
  id       String @id @default(cuid())
  buyerId  String
  kind     String
  fileUrl  String
  fileName String
  buyer    Buyer  @relation(fields: [buyerId], references: [id], onDelete: Cascade)

  @@index([buyerId])
}

model Transporter {
  id            String              @id @default(cuid())
  type          TransporterType
  legalName     String
  tradeName     String?
  npwp          String?             @unique
  pkpStatus     PkpStatus
  addressLine   String?
  city          String?
  province      String?
  postalCode    String?
  picName       String?
  picPhone      String?
  picEmail      String?
  bankName      String?
  bankAccountNo String?
  bankAccountNm String?
  statementUrl  String?
  isVerified    Boolean             @default(true)
  status        RecordStatus        @default(AKTIF)
  notes         String?
  createdAt     DateTime            @default(now())
  updatedAt     DateTime            @updatedAt
  drivers       Driver[]
  contracts     TransportContract[]
  tariffs       TransportTariff[]
  vehicles      Vehicle[]

  @@index([legalName])
  @@index([pkpStatus])
  @@index([status])
  @@index([city])
}

model Vehicle {
  id                 String              @id @default(cuid())
  transporterId      String
  plateNo            String              @unique
  type               String
  capacityTons       Decimal?            @db.Decimal(10, 2)
  stnkUrl            String?
  stnkValidThru      DateTime?
  kirUrl             String?
  kirValidThru       DateTime?
  gpsId              String?
  photoUrl           String?
  createdAt          DateTime            @default(now())
  updatedAt          DateTime            @updatedAt
  transporter        Transporter         @relation(fields: [transporterId], references: [id], onDelete: Cascade)
  weighbridgeTickets WeighbridgeTicket[]

  @@index([transporterId])
  @@index([plateNo])
}

model Driver {
  id            String      @id @default(cuid())
  transporterId String
  name          String
  phone         String?
  nik           String?
  simType       String?
  simUrl        String?
  simValidThru  DateTime?
  createdAt     DateTime    @default(now())
  updatedAt     DateTime    @updatedAt
  transporter   Transporter @relation(fields: [transporterId], references: [id], onDelete: Cascade)

  @@index([transporterId])
  @@index([name])
}

model TransportTariff {
  id            String      @id @default(cuid())
  transporterId String
  origin        String
  destination   String
  commodity     String
  unit          String
  price         Decimal     @db.Decimal(15, 2)
  includeToll   Boolean     @default(false)
  includeUnload Boolean     @default(false)
  includeTax    Boolean     @default(false)
  notes         String?
  createdAt     DateTime    @default(now())
  updatedAt     DateTime    @updatedAt
  transporter   Transporter @relation(fields: [transporterId], references: [id], onDelete: Cascade)

  @@index([transporterId, origin, destination, commodity])
}

model TransportContract {
  id            String      @id @default(cuid())
  transporterId String
  contractNo    String      @unique
  buyerId       String?
  commodity     String
  startDate     DateTime?
  endDate       DateTime?
  baseTariffId  String?
  dokUrl        String?
  createdAt     DateTime    @default(now())
  updatedAt     DateTime    @updatedAt
  transporter   Transporter @relation(fields: [transporterId], references: [id], onDelete: Cascade)

  @@index([transporterId])
  @@index([contractNo])
}

model Account {
  id                String             @id @default(cuid())
  companyId         String
  code              String
  name              String
  class             AccountClass
  normalSide        NormalSide
  isPosting         Boolean            @default(true)
  isCashBank        Boolean            @default(false)
  taxCode           TaxCode            @default(NON_TAX)
  currency          String?            @db.VarChar(10)
  description       String?            @db.VarChar(300)
  status            RecordStatus       @default(AKTIF)
  parentId          String?
  createdAt         DateTime           @default(now())
  updatedAt         DateTime           @updatedAt
  company           Company            @relation(fields: [companyId], references: [id], onDelete: Cascade)
  parent            Account?           @relation("AccountHierarchy", fields: [parentId], references: [id])
  children          Account[]          @relation("AccountHierarchy")
  journalLines      JournalLine[]
  openingBalances   OpeningBalance[]
  systemAccountMaps SystemAccountMap[]

  @@unique([companyId, code])
  @@index([companyId, class])
  @@index([parentId])
}

model SystemAccountMap {
  id        String           @id @default(cuid())
  companyId String
  key       SystemAccountKey
  accountId String
  createdAt DateTime         @default(now())
  updatedAt DateTime         @updatedAt
  account   Account          @relation(fields: [accountId], references: [id])
  company   Company          @relation(fields: [companyId], references: [id], onDelete: Cascade)

  @@unique([companyId, key])
  @@index([accountId])
}

model FiscalPeriod {
  id              String           @id @default(cuid())
  companyId       String
  year            Int
  month           Int
  startDate       DateTime
  endDate         DateTime
  isClosed        Boolean          @default(false)
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt
  company         Company          @relation(fields: [companyId], references: [id], onDelete: Cascade)
  openingBalances OpeningBalance[]

  @@unique([companyId, year, month])
  @@index([companyId, isClosed])
}

model OpeningBalance {
  id        String       @id @default(cuid())
  companyId String
  periodId  String
  accountId String
  debit     Decimal      @default(0) @db.Decimal(18, 2)
  credit    Decimal      @default(0) @db.Decimal(18, 2)
  createdAt DateTime     @default(now())
  updatedAt DateTime     @updatedAt
  account   Account      @relation(fields: [accountId], references: [id], onDelete: Cascade)
  company   Company      @relation(fields: [companyId], references: [id], onDelete: Cascade)
  period    FiscalPeriod @relation(fields: [periodId], references: [id], onDelete: Cascade)

  @@unique([companyId, periodId, accountId])
  @@index([accountId])
}

model PksMutuProduksi {
  id         String    @id @default(cuid())
  rowId      String    @unique
  tanggal    DateTime
  shift      String    @db.VarChar(10)
  payload    Json
  syncStatus String?   @db.VarChar(20)
  sentAt     DateTime?
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt

  @@unique([tanggal, shift])
  @@index([tanggal])
  @@index([shift])
}

model Unit {
  id                String             @id @default(cuid())
  code              String             @unique
  name              String
  isBase            Boolean            @default(false)
  conversionToBase  Decimal            @default(1.0) @db.Decimal(10, 4)
  description       String?
  isActive          Boolean            @default(true)
  createdAt         DateTime           @default(now())
  updatedAt         DateTime           @updatedAt
  goodsIssueLines   GoodsIssueLine[]
  goodsReceiptLines GoodsReceiptLine[]
  itemsAsBase       Item[]             @relation("ItemBaseUnit")
  itemsAsIssue      Item[]             @relation("ItemIssueUnit")
  itemRequestLines  ItemRequestLine[]

  @@index([name])
}

model Category {
  id          String     @id @default(cuid())
  code        String     @unique
  name        String
  description String?
  isActive    Boolean    @default(true)
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
  items       Item[]
  itemTypes   ItemType[]

  @@index([name])
}

model ItemType {
  id          String   @id @default(cuid())
  categoryId  String
  code        String
  name        String
  description String?
  isActive    Boolean  @default(true)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  items       Item[]
  category    Category @relation(fields: [categoryId], references: [id])

  @@unique([categoryId, code])
  @@index([categoryId])
  @@index([name])
}

model Warehouse {
  id            String         @id @default(cuid())
  code          String         @unique
  name          String
  address       String?
  isActive      Boolean        @default(true)
  createdAt     DateTime       @default(now())
  updatedAt     DateTime       @updatedAt
  bins          Bin[]
  goodsIssues   GoodsIssue[]
  goodsReceipts GoodsReceipt[]
  stockBalances StockBalance[]
  stockLedgers  StockLedger[]

  @@index([name])
}

model Bin {
  id            String         @id @default(cuid())
  warehouseId   String
  code          String
  name          String
  description   String?
  isActive      Boolean        @default(true)
  createdAt     DateTime       @default(now())
  updatedAt     DateTime       @updatedAt
  warehouse     Warehouse      @relation(fields: [warehouseId], references: [id], onDelete: Cascade)
  stockBalances StockBalance[]
  stockLedgers  StockLedger[]

  @@unique([warehouseId, code])
  @@index([warehouseId])
}

model Item {
  id                 String              @id @default(cuid())
  sku                String              @unique
  name               String
  description        String?
  categoryId         String
  itemTypeId         String
  baseUnitId         String
  defaultIssueUnitId String?
  valuationMethod    String              @default("AVERAGE")
  minStock           Decimal?            @db.Decimal(18, 4)
  maxStock           Decimal?            @db.Decimal(18, 4)
  isActive           Boolean             @default(true)
  createdAt          DateTime            @default(now())
  updatedAt          DateTime            @updatedAt
  goodsIssueLines    GoodsIssueLine[]
  goodsReceiptLines  GoodsReceiptLine[]
  baseUnit           Unit                @relation("ItemBaseUnit", fields: [baseUnitId], references: [id])
  category           Category            @relation(fields: [categoryId], references: [id])
  defaultIssueUnit   Unit?               @relation("ItemIssueUnit", fields: [defaultIssueUnitId], references: [id], onDelete: Restrict)
  itemType           ItemType            @relation(fields: [itemTypeId], references: [id])
  itemRequestLines   ItemRequestLine[]
  stockBalances      StockBalance[]
  stockLedgers       StockLedger[]
  weighbridgeTickets WeighbridgeTicket[]

  @@index([name])
  @@index([sku])
  @@index([categoryId])
  @@index([itemTypeId])
}

model StockBalance {
  id          String    @id @default(cuid())
  itemId      String
  warehouseId String
  binId       String?
  qtyOnHand   Decimal   @default(0) @db.Decimal(18, 4)
  avgCost     Decimal   @default(0) @db.Decimal(18, 4)
  updatedAt   DateTime  @updatedAt
  bin         Bin?      @relation(fields: [binId], references: [id])
  item        Item      @relation(fields: [itemId], references: [id], onDelete: Cascade)
  warehouse   Warehouse @relation(fields: [warehouseId], references: [id], onDelete: Cascade)

  @@unique([itemId, warehouseId, binId])
  @@index([itemId])
  @@index([warehouseId])
}

model StockLedger {
  id            String    @id @default(cuid())
  ts            DateTime  @default(now())
  itemId        String
  warehouseId   String
  binId         String?
  referenceType String
  referenceId   String
  qtyDelta      Decimal   @db.Decimal(18, 4)
  unitCost      Decimal?  @db.Decimal(18, 4)
  note          String?
  createdById   String
  createdAt     DateTime  @default(now())
  bin           Bin?      @relation(fields: [binId], references: [id])
  item          Item      @relation(fields: [itemId], references: [id], onDelete: Cascade)
  warehouse     Warehouse @relation(fields: [warehouseId], references: [id], onDelete: Cascade)

  @@index([itemId])
  @@index([warehouseId])
  @@index([referenceType])
  @@index([ts])
}

model GoodsReceipt {
  id          String             @id @default(cuid())
  docNumber   String             @unique
  date        DateTime
  warehouseId String
  sourceType  String
  sourceRef   String?
  loanIssueId String?
  note        String?
  glStatus    String             @default("PENDING")
  glPostedAt  DateTime?
  glEntryId   String?
  createdById String
  createdAt   DateTime           @default(now())
  updatedAt   DateTime           @updatedAt
  warehouse   Warehouse          @relation(fields: [warehouseId], references: [id])
  lines       GoodsReceiptLine[]

  @@index([date])
  @@index([warehouseId])
  @@index([sourceType])
  @@index([loanIssueId])
}

model GoodsReceiptLine {
  id              String       @id @default(cuid())
  receiptId       String
  itemId          String
  unitId          String
  qty             Decimal      @db.Decimal(18, 4)
  unitCost        Decimal?     @db.Decimal(18, 4)
  note            String?
  loanIssueLineId String?
  item            Item         @relation(fields: [itemId], references: [id])
  receipt         GoodsReceipt @relation(fields: [receiptId], references: [id], onDelete: Cascade)
  unit            Unit         @relation(fields: [unitId], references: [id])

  @@index([receiptId])
  @@index([itemId])
  @@index([loanIssueLineId])
}

model GoodsIssue {
  id                  String           @id @default(cuid())
  docNumber           String           @unique
  date                DateTime
  warehouseId         String
  purpose             String
  targetDept          String?
  pickerName          String?
  status              String           @default("APPROVED")
  note                String?
  loanReceiver        String?
  expectedReturnAt    DateTime?
  loanNotes           String?
  isLoanFullyReturned Boolean          @default(false)
  expenseAccountId    String?
  costCenter          String?
  glStatus            String           @default("PENDING")
  glPostedAt          DateTime?
  glEntryId           String?
  createdById         String
  createdAt           DateTime         @default(now())
  updatedAt           DateTime         @updatedAt
  warehouse           Warehouse        @relation(fields: [warehouseId], references: [id])
  lines               GoodsIssueLine[]

  @@index([date])
  @@index([warehouseId])
  @@index([status])
  @@index([purpose])
  @@index([glStatus])
}

model GoodsIssueLine {
  id          String     @id @default(cuid())
  issueId     String
  itemId      String
  unitId      String
  qty         Decimal    @db.Decimal(18, 4)
  unitCost    Decimal?   @db.Decimal(18, 4)
  note        String?
  qtyReturned Decimal    @default(0) @db.Decimal(18, 4)
  issue       GoodsIssue @relation(fields: [issueId], references: [id], onDelete: Cascade)
  item        Item       @relation(fields: [itemId], references: [id])
  unit        Unit       @relation(fields: [unitId], references: [id])

  @@index([issueId])
  @@index([itemId])
}

model ItemRequest {
  id             String            @id @default(cuid())
  reqNumber      String            @unique
  date           DateTime
  requestDept    String
  reason         String?
  status         String            @default("DRAFT")
  relatedFunding String?
  createdById    String
  createdAt      DateTime          @default(now())
  updatedAt      DateTime          @updatedAt
  lines          ItemRequestLine[]

  @@index([date])
  @@index([status])
}

model ItemRequestLine {
  id        String      @id @default(cuid())
  requestId String
  itemId    String
  unitId    String
  qty       Decimal     @db.Decimal(18, 4)
  item      Item        @relation(fields: [itemId], references: [id])
  request   ItemRequest @relation(fields: [requestId], references: [id], onDelete: Cascade)
  unit      Unit        @relation(fields: [unitId], references: [id])

  @@index([requestId])
  @@index([itemId])
}

model JournalEntry {
  id          String        @id @default(cuid())
  companyId   String
  entryNumber String        @unique
  date        DateTime
  sourceType  String
  sourceId    String?
  memo        String?
  status      String        @default("POSTED")
  postedAt    DateTime?
  postedById  String?
  createdById String
  createdAt   DateTime      @default(now())
  updatedAt   DateTime      @updatedAt
  company     Company       @relation(fields: [companyId], references: [id], onDelete: Cascade)
  lines       JournalLine[]

  @@index([companyId, date])
  @@index([sourceType, sourceId])
  @@index([status])
}

model JournalLine {
  id          String       @id @default(cuid())
  entryId     String
  accountId   String
  debit       Decimal      @default(0) @db.Decimal(18, 2)
  credit      Decimal      @default(0) @db.Decimal(18, 2)
  costCenter  String?
  dept        String?
  itemId      String?
  warehouseId String?
  description String?
  account     Account      @relation(fields: [accountId], references: [id])
  entry       JournalEntry @relation(fields: [entryId], references: [id], onDelete: Cascade)

  @@index([entryId])
  @@index([accountId])
  @@index([costCenter])
}

model WeighbridgeTicket {
  id                      String      @id @default(cuid())
  companyId               String
  noSeri                  String
  vehicleId               String
  supplierId              String
  itemId                  String
  tanggal                 DateTime
  jamMasuk                DateTime
  jamKeluar               DateTime?
  timbang1                Decimal     @db.Decimal(12, 2)
  timbang2                Decimal     @db.Decimal(12, 2)
  netto1                  Decimal     @db.Decimal(12, 2)
  potPercent              Decimal     @db.Decimal(5, 4)
  potKg                   Decimal     @db.Decimal(12, 2)
  beratTerima             Decimal     @db.Decimal(12, 2)
  lokasiKebun             String?
  penimbang               String?
  hargaPerKg              Decimal     @default(0) @db.Decimal(12, 2)
  pphRate                 Decimal     @default(0) @db.Decimal(5, 4)
  upahBongkarPerKg        Decimal     @default(0) @db.Decimal(12, 2)
  total                   Decimal     @default(0) @db.Decimal(14, 2)
  totalPph                Decimal     @default(0) @db.Decimal(14, 2)
  totalUpahBongkar        Decimal     @default(0) @db.Decimal(14, 2)
  totalPembayaranSupplier Decimal     @default(0) @db.Decimal(14, 2)
  status                  DocStatus   @default(DRAFT)
  purchaseJeId            String?
  unloadJeId              String?
  createdAt               DateTime    @default(now())
  updatedAt               DateTime    @updatedAt
  createdById             String
  company                 Company     @relation(fields: [companyId], references: [id], onDelete: Cascade)
  vehicle                 Vehicle     @relation(fields: [vehicleId], references: [id])
  supplier                SupplierTBS @relation(fields: [supplierId], references: [id])
  item                    Item        @relation(fields: [itemId], references: [id])

  @@unique([companyId, noSeri])
  @@index([tanggal])
  @@index([status])
  @@index([vehicleId])
  @@index([supplierId])
  @@index([itemId])
}

enum UserRole {
  GROUP_VIEWER
  EXECUTIVE
  PT_MANAGER
  PT_NILO_ADMIN
  PT_ZTA_ADMIN
  PT_TAM_ADMIN
  PT_HTK_ADMIN
  PT_PKS_ADMIN
  UNIT_SUPERVISOR
  TECHNICIAN
  OPERATOR
  HR
  FINANCE_AR
  FINANCE_AP
  GL_ACCOUNTANT
}

enum SupplierType {
  RAMP_PERON
  KUD
  KELOMPOK_TANI
}

enum PajakPKP {
  PKP_11_PERSEN
  PKP_1_1_PERSEN
  NON_PKP
}

enum FamilyRelation {
  ISTRI
